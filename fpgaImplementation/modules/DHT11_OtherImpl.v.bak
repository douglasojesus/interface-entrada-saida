module DHT11_OtherImpl(clock, reset, transmition_line, data_valid);

	input clock, reset;
	inout transmition_line;
	output reg [31:0] data_valid;
	
	parameter	POWER_ON_NUM 	= 1_000_000,
					S_POWER_ON   	= 3'd0,
					S_LOW_20MS		= 3'd1,
					S_HIGH_13US		= 3'd2,
					S_LOW_83US		= 3'd3,
					S_HIGH_87US		= 3'd4,
					S_SEND_DATA		= 3'd5,
					S_DEALY			= 3'd6;
					
	



endmodule